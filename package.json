{
  "name": "@racsodev/cv-pdf-to-json",
  "version": "2.2.3",
  "description": "Node.js library that extracts CV/resume data from PDF files and converts it to structured JSON using Claude's native PDF support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "process": "ts-node src/devProcessCmd.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "cv",
    "resume",
    "extract",
    "claude",
    "ai",
    "pdf",
    "json",
    "typescript"
  ],
