    "@anthropic-ai/sdk": "^0.30.1"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/racso-dev/cv-pdf-to-json.git"
  },
  "directories": {
    "doc": "docs"
